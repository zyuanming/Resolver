# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Resolver-iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Resolver-iOS

  target 'Resolver-iOSTests' do
    inherit! :search_paths
    # Pods for testing
    platform :ios, '9.0'

    pod 'Quick'
    pod 'Nimble'
  end

end

target 'Resolver-macOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Resolver-macOS

  target 'Resolver-macOSTests' do
    inherit! :search_paths
    # Pods for testing
    platform :osx, '10.11'

    pod 'Quick'
    pod 'Nimble'
  end

end
